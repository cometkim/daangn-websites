    "lint": "biome check . --apply",
    "format": "biome format . --write"
  },
  "resolutions": {
    "sharp": "0.33.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.1",
    "@types/node": "^20.11.28",
    "typescript": "^5.4.2",
    "ultra-runner": "^3.10.5"
  },
  "packageManager": "yarn@4.2.2"
}
